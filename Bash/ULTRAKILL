#!/bin/bash
RED='\033[0;31m'
NC='\033[0m' 
echo -e "${RED}\n _   _  _    _____  ____      _     _  __  _   _      _     "
echo -e "${RED}| | | || |  |_ / _||  _ \    //\   | |/ / | | | |    | |    "
echo -e "${RED}| | | || |    | |  | |_) |  / _ \  | |_/  |•| | |    | |    "
echo -e "${RED}| |_| || |___ | |  |  _ <  / __\ \ | | \  |•| |\|___ | |___ "
echo -e "${RED} \___/ |_____||_|  |_| \_\/_/   \_\|_|\_\ |_| |_\___||___/_|${NC}"